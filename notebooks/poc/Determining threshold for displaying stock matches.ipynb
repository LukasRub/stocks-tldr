   ],
   "source": [
    "x = np.linspace(-0.4, 1, 1000)\n",
    "y = threshold_func(x)\n",
    "\n",
    "ax = plt.figure(figsize=(12, 8))\n",
    "plt.plot(x, y, \"-\")\n",
